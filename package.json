{
  "name": "MyPlayer",
  "version": "0.0.1",
  "description": "Player for Music. Simple, clean and easy.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run": "electron ./src/",
    "compile": "cp -R ./src/* ./_bins/electron-v0.30.1-darwin-x64/MyPlayer.app/Contents/Resources/app",
    "buildwin": "electron-packager ./src MyPlayer --out=./_bins/ --asar=true --icon=./src/img/myplayer.ico --platform=win32 --arch=all --version=0.25.1 --overwrite",
    "buildmac": "electron-packager ./src MyPlayer --out=./_bins/ --asar=true --icon=./src/img/myplayer.icns --platform=darwin --arch=x64 --version=0.25.1 --overwrite && cp ./src/osx/Info.plist ./_bins/MyPlayer-darwin-x64/MyPlayer.app/Contents/",
    "buildlinux": "electron-packager ./src MyPlayer --out=./_bins/ --asar=true --platform=linux --arch=all --version=0.25.1 --overwrite"
  },
  "keywords": [
    "music",
    "player"
  ],
  "author": "Robert Agthe",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^5.0.1"
  },
  "dependencies": {
  }
}
