{
  "name": "myplayer",
  "version": "0.5.1",
  "description": "Player for Music. Simple, clean and easy.",
  "main": "src/main.js",
  "build": {
    "appId": "com.github.electron.myplayer",
    "asar": false,
    "mac": {
      "category": "public.app-category.music"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -w -m -l",
    "dist:single": "electron-builder --win portable"
  },
  "keywords": [
    "music",
    "player",
    "electron",
    "winamp",
    "nodeamp"
  ],
  "author": { 
    "name" : "Robert Agthe",
    "email" : "robert@scriptshit.de",
    "url" : "http://scriptshit.de"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/polarity/MyPlayer.git"
  },
  "bugs": {
    "url": "https://github.com/polarity/MyPlayer/issues"
  },
  "devDependencies": {
    "electron-builder": "*",
    "electron": "^40.6.0"
  },
  "dependencies": {
    "electron-plugins": "0.0.4",
    "electron-updater": "^6.8.3",
    "font-awesome": "^4.4.0",
    "id3js": "^1.1.3",
    "lodash": "^3.10.1",
    "recursive-readdir-sync": "^1.0.6",
    "wavesurfer.js": "^1.0.30"
  }
}
